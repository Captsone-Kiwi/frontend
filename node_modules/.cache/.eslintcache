({ ...reserveInfo, [name]: value });\n  };\n\n  //면접관 리스트\n  const [currViewer, setCurrViewer] = useState(0);\n  const interviewerList = (e, index) => {\n    setReserveInfo({\n      ...reserveInfo,\n      [e.target.name]: [\n        ...reserveInfo[e.target.name].slice(0, index),\n        e.target.value,\n        ...reserveInfo[e.target.name].slice(index + 1),\n      ],\n    });\n    setCurrViewer(index);\n  };\n  const addInterviewer = ({ target }) => {\n    const name = target.getAttribute(\"name\");\n    setCurrViewer(currViewer + 1);\n    setReserveInfo({\n      ...reserveInfo,\n      [name]: [...reserveInfo[name], \"\"],\n    });\n  };\n  const removeInterviewer = (e, index) => {\n    const name = e.target.getAttribute(\"name\");\n    if (reserveInfo.interviewer.length > 1) {\n      setCurrViewer(currViewer - 1);\n      setReserveInfo({\n        ...reserveInfo,\n        [name]: [\n          ...reserveInfo[name].filter((value, idx) => {\n            return idx !== index;\n          }),\n        ],\n      });\n    } else {\n      alert(\"더이상 삭제할 수 없습니다.\");\n    }\n  };\n\n  function InterviewerInputFrom(props) {\n    useEffect(() => {\n      const input = document.querySelector(`.viewer-input${props.index}`);\n      input.value = reserveInfo.interviewer[props.index];\n      const currInput = document.querySelector(`.viewer-input${currViewer}`);\n      currInput.focus();\n    }, []);\n    return (\n      <style.inputBox>\n        {console.log(\"currViewer\", currViewer)}\n        <style.interviewerList\n          name=\"interviewer\"\n          className={\"viewer-input\" + props.index}\n          onChange={(e) => {\n            interviewerList(e, props.index);\n          }}\n          InputProps={{ disableUnderline: true }}\n        />\n        <style.removeBtn\n          name=\"interviewer\"\n          onClick={(e) => {\n            removeInterviewer(e, props.index);\n          }}\n        >\n          <style.removeImg\n            name=\"interviewer\"\n            src=\"/images/common/removeBtn.png\"\n          />\n        </style.removeBtn>\n      </style.inputBox>\n    );\n  }\n\n  return (\n    <style.mainContainer>\n      <SideMenu side={side} setSide={setSide} />\n      <style.interviewContainer>\n        {step === 0 ? (\n          // 예정 면접들 보여주는 부분들\n          <InterviewList\n            step={step}\n            setStep={setStep}\n            prevStep={prevStep}\n            nextStep={nextStep}\n            tag={tag}\n            setTag={setTag}\n          />\n        ) : (\n          // 면접 예약 버튼 눌렀을 때 실행될 부분들\n          <style.Container>\n            <style.prevBtn onClick={prevStep}>\n              <style.prevImg src=\"/images/common/prevBtn.png\" />\n              면접으로 돌아가기\n            </style.prevBtn>\n            <style.Span>면접 예약</style.Span>\n            <style.reserveContainer>\n              <style.reserveSection>\n                <style.reserveTitle>면접명</style.reserveTitle>\n                <style.reserveName\n                  name=\"interviewName\"\n                  type=\"text\"\n                  onChange={reserveUpload}\n                  required\n                  autoFocus\n                  InputProps={{ disableUnderline: true }}\n                />\n              </style.reserveSection>\n              <style.reserveSection>\n                <style.reserveTitle>면접 일시</style.reserveTitle>\n              </style.reserveSection>\n              <style.reserveSection>\n                <style.reserveTitle>면접 ID</style.reserveTitle>\n              </style.reserveSection>\n              <style.reserveSection>\n                <style.reserveTitle>템플릿</style.reserveTitle>\n              </style.reserveSection>\n              <style.reserveSection>\n                <style.reserveTitle>면접관</style.reserveTitle>\n                <style.detailContainer>\n                  {reserveInfo.interviewer.map((e, index) => (\n                    <InterviewerInputFrom\n                      name=\"interviewer\"\n                      index={index}\n                      key={index}\n                    />\n                  ))}\n                  <style.addBtn name=\"interviewer\" onClick={addInterviewer}>\n                    <style.addImg\n                      name=\"interviewer\"\n                      src=\"/images/common/addBtn.png\"\n                    />\n                  </style.addBtn>\n                </style.detailContainer>\n              </style.reserveSection>\n              <style.reserveSection>\n                <style.reserveTitle>면접자</style.reserveTitle>\n              </style.reserveSection>\n            </style.reserveContainer>\n          </style.Container>\n        )}\n      </style.interviewContainer>\n    </style.mainContainer>\n  );\n}\n\nexport default Interview;\n","/Users/kimchanmi/Capstone-Kiwi/frontend/src/pages/Interview/styles.js",[],[],["129","130"],"/Users/kimchanmi/Capstone-Kiwi/frontend/src/pages/Interview/interviewList.js",[],[],["131","132"],"/Users/kimchanmi/Capstone-Kiwi/frontend/src/pages/Interview/listStyles.js",[],[],{"ruleId":"133","replacedBy":"134"},{"ruleId":"135","replacedBy":"136"},{"ruleId":"137","severity":1,"message":"138","line":76,"column":8,"nodeType":"139","endLine":76,"endColumn":10,"suggestions":"140"},{"ruleId":"133","replacedBy":"141"},{"ruleId":"135","replacedBy":"142"},{"ruleId":"133","replacedBy":"143"},{"ruleId":"135","replacedBy":"144"},"no-native-reassign",["145"],"no-negated-in-lhs",["146"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'props.index'. Either include it or remove the dependency array.","ArrayExpression",["147"],["145"],["146"],["145"],["146"],"no-global-assign","no-unsafe-negation",{"desc":"148","fix":"149"},"Update the dependencies array to be: [props.index]",{"range":"150","text":"151"},[2163,2165],"[props.index]"]